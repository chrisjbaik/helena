{
  "manifest_version": 2,
  "name": "Helena Scraper and Automator",
  "description": "This extension uses the Helena web automation language to scrape data from well-structured webpages.",
  "version": "0.0.0.16",
  "browser_action": {
    "default_icon": "icons/helenaH.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "file://*/*",
    "webRequest",
    "storage",
    "unlimitedStorage",
    "webRequestBlocking",
    "system.display",
    "system.memory",
    "webNavigation"
  ],
  "background": {
    "page": "pages/background.html"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*", "file://*/*"],
      "match_about_blank": true,
      "all_frames": true,
      "js": ["dist/content.js"]
    }
  ],
  "old_web_accessible_resources": [
    "code/lib/jquery.js",
    "code/lib/jquery_csv.js",
    "code/lib/jquery-ui.js",
    "code/lib/jquery-dateFormat.min.js",
    "code/lib/JSOG.js"
  ]
}
